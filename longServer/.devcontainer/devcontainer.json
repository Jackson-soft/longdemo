{
	"name": "C++",
	"dockerFile": "Dockerfile",
	"extensions": [
		"ms-vscode.cpptools",
		"ms-vscode.cmake-tools",
		"visualstudioexptteam.vscodeintellicode",
		"twxs.cmake",
		"xaver.clang-format",
		"lfs.vscode-emacs-friendly",
		"tabnine.tabnine-vscode"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	]
}